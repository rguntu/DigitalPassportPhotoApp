{
    "cli": {
        "version": ">= 16.20.1",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
            },
            "cache": {
                "key": "v2",
                "paths": [
                    "node_modules",
                    "ios/Pods"
                ]
            }
        },
        "simulator": {
            "developmentClient": true,
            "distribution": "internal",
            "ios": {
                "simulator": true
            },
            "env": {
                "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
            },
            "cache": {
                "key": "v2",
                "paths": [
                    "node_modules",
                    "ios/Pods"
                ]
            }
        },
        "preview": {
            "distribution": "internal",
            "env": {
                "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
            },
            "cache": {
                "key": "v2",
                "paths": [
                    "node_modules",
                    "ios/Pods"
                ]
            }
        },
        "production": {
            "autoIncrement": true,
            "env": {
                "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
            },
            "cache": {
                "key": "v2",
                "paths": [
                    "node_modules",
                    "ios/Pods",
                    "~/.gradle/caches"
                ]
            }
        }
    },
    "submit": {
        "production": {}
    }
}

